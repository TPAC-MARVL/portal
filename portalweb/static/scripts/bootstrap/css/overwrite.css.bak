html {
  height: 100%;
}

body {
  height: 100%;
  font-family: "Helvetica Neue", Helvetica, Arial, sans-seri;
}

.navbar {
  margin-bottom: 5px;
}

.nav-list > .active > a, .nav-list > .active > a:hover, .nav-list > .active > a:focus {
  background-color: #033C73;
}

a {
  color: #0a6E87;
}

.row-fluid .span10 {
  width: 84.55%;
}

.row-fluid [class*="span"] {
  margin-left: 0.5%;
}

.container-fluid {
  padding: 0px;
}

.navbar-inner {
  padding-left: 0px;
  padding-right: 0px;
}

